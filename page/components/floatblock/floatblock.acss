.wen-float-block-box{
  position: fixed;
  /* transition: all 1s; */
  overflow: hidden;
  background: blue;
  height: 10vw;
  display: flex;
  justify-content: center;
  align-items: center;
  white-space: nowrap;
  color: white;
}
.wen-float-block-demo{
  /* width: 100vw; */
  height: 10vw;
  display: flex;
  justify-content: center;
  align-items: center;
  color: white;
  font-size: 4vw;
  /* background: ""; */
}
.wen-float-block-demo-text-right {
  text-align: right;
}

